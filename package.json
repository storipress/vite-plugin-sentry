{
  "name": "@storipress/vite-plugin-sentry",
  "description": "The unofficial Sentry plugin for Vite ⚡️",
  "version": "1.0.15",
  "author": "ikenfin",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "https://github.com/storipress/vite-plugin-sentry",
  "repository": {
    "type": "git",
    "url": "https://github.com/storipress/vite-plugin-sentry"
  },
  "files": [
    "index.d.ts",
    "client.d.ts",
    "types/*",
    "dist/*"
  ],
  "keywords": [
    "vite-plugin",
    "sentry",
    "sentry-cli",
    "sourcemap"
  ],
  "scripts": {
    "bundle": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=develop rollup -c",
    "test": "jest test/**.test.ts",
    "test:coverage": "jest --coverage test/**.test.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:tests": "eslint ./test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/node": "17.0.35",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "rollup": "2.75.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "0.31.2",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@sentry/cli": "2.1.0"
  },
  "peerDependencies": {
    "vite": "^2.9.9"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  }
}
